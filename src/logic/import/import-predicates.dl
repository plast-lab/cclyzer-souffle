constant_in_function(as(const, Constant), as(function, Function)) :-
   _constant_in_function(const, functionName),
   function_has_name(function, functionName).

variable_in_function(as(var, Variable), as(function, Function)) :-
   _variable_in_function(var, functionName),
   function_has_name(function, functionName).

